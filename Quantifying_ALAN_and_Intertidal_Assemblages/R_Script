  theme(plot.title = element_text(size = rel(1.5), angle = 00)) + 
  theme(axis.text.x = element_text(size = 12, angle = 00),
        axis.text.y = element_text(size = 12, angle = 90))+
  theme(strip.text.x = element_text(size = 12))
Mobile_N_Sp

